[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/root/ieeewebsite/
ExecStart=/usr/lib/python3/dist-packages/gunicorn --access-logfile gunicorn.log --workers 3 --bind unix:/ieeewebsite/ieeewebsite.sock ieeewebsite.wsgi:application

[Install]
WantedBy=multi-user.target